---
interface Props {
  size?: number;
  class?: string;
}

const { size = 24, class: className = "" } = Astro.props;
---

<svg
  width={size}
  height={size}
  viewBox="0 0 24 24"
  fill="none"
  stroke="currentColor"
  stroke-width="2"
  stroke-linecap="round"
  stroke-linejoin="round"
  class={className}
>
  <rect x="4.5" y="2" width="15" height="20" rx="7.5" ry="7.5"></rect>
  <line x1="4.5" y1="12" x2="19.5" y2="12"></line>
</svg>
